{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      "expo-apple-authentication"
    ],
    "name": "notetest-app",
    "slug": "notetest-app",
    "android": {
      "package": "com.anonymous.notetestapp",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBhn70iCWfX0amrbjxioM4JEKVmFERzozI"
        }
      }
    },
    "newArchEnabled": true
  }
}
